   "source": [
    "def genfib (n): \n",
    "    a=0\n",
    "    yield a \n",
    "    b=1\n",
    "    yield b \n",
    "    while b < n:\n",
    "        a, b = b, a+b \n",
    "        yield b"
   ]
  },
